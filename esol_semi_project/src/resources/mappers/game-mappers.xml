<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="gameMapper">
<resultMap type="GameInfoDto" id="GameInfoDtoResultMap">
	<result column="GAME_TITLE" property="gameTitle"/>
	<result column="GAME_PRICE" property="gamePrice"/>
	<result column="GAME_TAG" property="gameTag"/>
</resultMap>

<select id="allGameInfo" parameterType="" resultMap="GameInfoDtoResultMap">
	select game_title,game_price,game_tag 
    	from game;
</select>
</mapper>
